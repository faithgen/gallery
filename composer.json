{
    "name": "faithgen/gallery",
    "description": "This is the Gallery plugin for FaithGen system",
    "type": "library",
    "require": {
        "php": "^7.4",
        "intervention/image": "^2.5",
        "faithgen/sdk": "^2.0",
        "crazybooot/base64-validation": "^1.0",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/queue": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Innocent Mazando",
            "email": "innocentmazando@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FaithGen\\Gallery\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FaithGen\\Gallery\\Providers\\GalleryServiceProvider",
                "FaithGen\\Gallery\\Providers\\AuthServiceProvider",
                "FaithGen\\Gallery\\Providers\\EventServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
